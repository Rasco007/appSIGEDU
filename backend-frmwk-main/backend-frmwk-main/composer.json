{
    "name": "tdi/misgt",
    "description": "Asistente de Gestion Tributaria movil - APIRest",
    "license": "MIT",
    "type": "project",
    "homepage": "https://tdi.com.ar",
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "4.4.1",
        "firebase/php-jwt": "6.8.1",
        "mpdf/mpdf": "8.2.0",
        "mpdf/qrcode": "1.2.0"
    },
    "require-dev": {
        "fakerphp/faker": "1.23.0",
        "mikey179/vfsstream": "1.6.11",
        "phpunit/phpunit": "9.6.12"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
